                  </div>
            </nav>
  <div class='container'>
    <div class='container'>
      <%= form_for @user do |f| %>
        <%= f.label :first_name %>
        <%= f.text_field :first_name %>
        <%= f.label :last_initial %>
        <%= f.text_field :last_initial %>
        <%= f.label 'Victor/Chad' %>
        <%= f.text_field :v_or_c %>
        <%= f.label :password %>
        <%= f.password_field_tag :password %>
        <%= f.submit 'Sign Up' %>
      <% end %>
    </div>
  </div>
